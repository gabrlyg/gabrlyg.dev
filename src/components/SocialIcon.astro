---
import { Instagram, Twitter, GitHub } from 'astro-feather-icons';
import type { SocialType } from '../types/types';

export interface Props {
  type: SocialType;
}

const socialIcons = {
  instagram: Instagram,
  twitter: Twitter,
  github: GitHub,
};

const { type } = Astro.props;

const SocialIcon = socialIcons[type];
---

<SocialIcon class='transition-colors' />
